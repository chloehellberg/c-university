@{
  Layout = "_Layout";
}

@model University.Models.Department
@Html.ActionLink(" All Departments", "Index", null, new { @class="btn btn-info fa fa-bank" })
<h4>Add Department</h4>

@using (Html.BeginForm())
{
  <table>
    <tr>
      <td>@Html.LabelFor(model => model.DepartmentName)</td>
      <td>@Html.TextBoxFor(model => model.DepartmentName)</td>
    </tr>
  </table>
  <button type="submit" class="btn btn-info fa" ><i class="fa-plus-circle"> Add</i></button>
}
